#!/bin/sh
bold="$(tput bold)"
normal="$(tput sgr0)"
if hash mamba 2> /dev/null; then
    echo mamba
elif hash micromamba 2> /dev/null; then
    echo micromamba
else
    echo echo "âŒ  ${bold}Could not find micromamba or mamba, please install and try again.${normal}" >&2
    echo -n echo "${bold}FAILED:${normal} mamba "
fi
